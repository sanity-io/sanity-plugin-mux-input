{
  "name": "sanity-plugin-mux-input",
  "version": "2.15.0",
  "description": "An input component that integrates Sanity Studio with Mux video encoding/hosting service.",
  "keywords": [
    "sanity",
    "video",
    "mux",
    "input",
    "plugin",
    "sanity-plugin",
    "media"
  ],
  "homepage": "https://github.com/sanity-io/sanity-plugin-mux-input#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-plugin-mux-input/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/sanity-plugin-mux-input.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "source": "./src/_exports/index.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist",
    "sanity.json",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "dev": "sanity dev",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepare": "husky install || true",
    "prepublishOnly": "npm run build",
    "test": "npm run lint && npm run type-check && npm run build",
    "type-check": "tsc --noEmit",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@mux/mux-player-react": "^3.11.5",
    "@mux/upchunk": "^3.5.0",
    "@sanity/icons": "^3.7.4",
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/ui": "^2.16.22",
    "@sanity/uuid": "^3.0.2",
    "iso-639-1": "^3.1.5",
    "jsonwebtoken-esm": "^1.0.5",
    "lodash": "^4.17.23",
    "react-rx": "^4.2.2",
    "rxjs": "^7.8.2",
    "scroll-into-view-if-needed": "^3.1.0",
    "suspend-react": "^0.1.3",
    "swr": "^2.4.0",
    "type-fest": "^4.41.0",
    "use-error-boundary": "^2.0.6"
  },
  "devDependencies": {
    "@sanity/client": "^7.16.0",
    "@sanity/pkg-utils": "^8.1.29",
    "@sanity/plugin-kit": "^4.0.20",
    "@sanity/semantic-release-preset": "^5.0.0",
    "@sanity/vision": "^3.99.0",
    "@types/lodash": "^4.17.24",
    "@types/react": "^19.2.14",
    "@types/react-is": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-sanity": "^7.1.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "npm-run-all2": "^6.2.6",
    "prettier": "^3.8.1",
    "prettier-plugin-packagejson": "^2.5.22",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-is": "^19.2.4",
    "sanity": "^3.99.0",
    "semantic-release": "^24.2.9",
    "styled-components": "^6.3.11",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "react": "^18.3 || ^19",
    "react-is": "^18.3 || ^19",
    "sanity": "^3.42.0 || ^4.0.0-0 || ^5.0.0",
    "styled-components": "^5 || ^6"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "sanityExchangeUrl": "https://www.sanity.io/plugins/sanity-plugin-mux-input",
  "browserslist": "extends @sanity/browserslist-config",
  "sanityPlugin": {
    "verifyPackage": {
      "srcIndex": false
    }
  }
