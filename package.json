{
  "name": "sanity-plugin-mux-input",
  "version": "2.0.0",
  "description": "An input component that integrates Sanity Studio with Mux video encoding/hosting service.",
  "keywords": [
    "sanity",
    "video",
    "mux",
    "input",
    "plugin",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/sanity-io/sanity-plugin-mux-input#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-plugin-mux-input/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/sanity-plugin-mux-input.git"
  },
  "browserslist": [
    "supports es6-module-dynamic-import"
  ],
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "exports": {
    ".": "./lib/esm/index.js",
    "./package.json": "./package.json"
  },
  "type": "module",
  "module": "./lib/esm/index.js",
  "source": "./src/index.tsx",
  "types": "./lib/types/index.d.ts",
  "files": [
    "lib",
    "sanity.json",
    "v2-incompatible.js"
  ],
  "scripts": {
    "prebuild": "npm run clean && plugin-kit verify-package --silent",
    "build": "parcel build --no-cache",
    "clean": "rimraf lib",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepare": "husky install || true",
    "prepublishOnly": "npm run build",
    "test": "npm run lint && npm run type-check && npm run build",
    "type-check": "tsc --noEmit",
    "watch": "parcel watch"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@mux/playback-core": "^0.9.0",
    "@mux/upchunk": "^2.4.0",
    "@reach/auto-id": "^0.17.0",
    "@sanity/icons": "^1.3.1",
    "@sanity/incompatible-plugin": "^0.0.1-studio-v3.1",
    "@sanity/uuid": "^3.0.1",
    "@swc/helpers": "^0.4.3",
    "classnames": "^2.3.1",
    "hls.js": "^1.1.5",
    "jsonwebtoken-esm": "^1.0.3",
    "media-chrome": "^0.8.1",
    "motion": "^10.13.1",
    "rxjs": "^6",
    "scroll-into-view-if-needed": "^2.2.29",
    "suspend-react": "^0.0.8",
    "swr": "^1.3.0",
    "use-device-pixel-ratio": "^1.1.0",
    "use-error-boundary": "^2.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@parcel/packager-ts": "^2.6.2",
    "@parcel/transformer-typescript-types": "^2.6.2",
    "@sanity/eslint-config-no-v2-imports": "^0.0.1-studio-v3.3",
    "@sanity/plugin-kit": "^0.1.0-v3-studio.1",
    "@sanity/ui": "^0.37.12",
    "@types/react": "^17",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "parcel": "^2.6.2",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.18",
    "prop-types": "^15.8.1",
    "react": "^17",
    "react-is": "^17",
    "sanipack": "^2.1.0",
    "sanity": "dev-preview",
    "semantic-release": "^19.0.3",
    "styled-components": "^5.3.5",
    "type-fest": "^2.17.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@sanity/ui": "^0.37 || latest",
    "prop-types": "^15",
    "react": "^17 || ^18",
    "react-is": "^17 || ^18",
    "sanity": "dev-preview || ^3",
    "styled-components": "^5.2.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sanityExchangeUrl": "https://www.sanity.io/plugins/sanity-plugin-mux-input",
  "sanityPlugin": {
    "verifyPackage": {
      "eslintImports": false,
      "module": false
    }
  }
}
